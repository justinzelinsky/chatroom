{
  "name": "chatroom",
  "version": "1.0.0",
  "private": true,
  "description": "Chatroom built with React, Redux, and Socket.IO",
  "main": "src/server/index.js",
  "scripts": {
    "analyze": "WEBPACK_ANALYZE=true npm run build",
    "docker": "npm run build && docker-compose up",
    "build": "NODE_ENV=production webpack",
    "start": "concurrently \"npm:watch-js\" \"npm:watch-server\"",
    "start:prod": "npm run build && NODE_PATH=src/server node src/server/index.js",
    "test": "jest",
    "tracker": "WEBPACK_TRACKER=true npm run build",
    "watch-js": "NODE_ENV=development webpack-dev-server --hot --host 0.0.0.0",
    "watch-server": "NODE_PATH=src/server nodemon src/server/index.js"
  },
  "author": "Justin Zelinsky",
  "repository": {
    "type": "git",
    "url": "git://github.com/justinzelinsky/chatroom.git"
  },
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.4.0",
    "classnames": "^2.2.6",
    "connect-mongo": "^3.1.2",
    "connected-react-router": "^6.6.1",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "mongoose": "^5.7.12",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "push.js": "^1.0.12",
    "react": "^16.11.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-logic": "^2.1.1",
    "reselect": "^4.0.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@packtracker/webpack-plugin": "^2.2.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-react-css-modules": "^5.2.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.7.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-jest": "^23.0.3",
    "eslint-plugin-react": "^7.16.0",
    "fetch-mock": "^8.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-fetch": "^2.6.1",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-scss": "^2.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.2.1",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
}
